---
layout: none
---
<root>
  <latest>
    <fullVersion>1.{{ site.data.installer.lesta.major }}.{{ site.data.installer.lesta.middle }}.{{ site.data.installer.lesta.minor }}#{{ site.data.installer.lesta.build }}</fullVersion>
    <major>{{ site.data.installer.lesta.major }}</major>
    <middle>{{ site.data.installer.lesta.middle }}</middle>
    <minor>{{ site.data.installer.lesta.minor }}</minor>
    <build>{{ site.data.installer.lesta.build }}</build>
    <link>{{ site.data.installer.lesta.link }}</link>
    {%- assign n = 0 -%}
    {%- for i in site.data.installer.lesta.mods -%}
      {%- if i == 0 %}{% continue %}{% endif -%}
      {%- assign j = 1 -%}
      {%- for m in (1..forloop.index0) -%}
        {%- assign j = j | times: 2 -%}
      {%- endfor -%}
      {%- assign n = n | plus: j -%}
    {%- endfor %}
    <mods>{{ n }}</mods>
  </latest>
  <sd_models>
    <fullVersion>1.{{ site.data.installer.sd-models.major }}.{{ site.data.installer.sd-models.middle }}.{{ site.data.installer.sd-models.minor }}#{{ site.data.installer.sd-models.build }}</fullVersion>
    <major>{{ site.data.installer.sd-models.major }}</major>
    <middle>{{ site.data.installer.sd-models.middle }}</middle>
    <minor>{{ site.data.installer.sd-models.minor }}</minor>
    <build>{{ site.data.installer.sd-models.build }}</build>
    <link>{{ site.data.installer.sd-models.link }}</link>
    {%- assign n = 0 -%}
    {%- for i in site.data.installer.sd-models.mods -%}
      {%- if i == 0 %}{% continue %}{% endif -%}
      {%- assign j = 1 -%}
      {%- for m in (1..forloop.index0) -%}
        {%- assign j = j | times: 2 -%}
      {%- endfor -%}
      {%- assign n = n | plus: j -%}
    {%- endfor %}
    <mods>{{ n }}</mods>
  </sd_models>
</root>